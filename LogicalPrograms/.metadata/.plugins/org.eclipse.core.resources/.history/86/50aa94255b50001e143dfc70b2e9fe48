package com.arraysprograms;


// Find Greatest Element in the Given Array

public class FindGreastestEle {
	public static void main(String[] args) {
		int[] a= {7,3,2,5,6};
		int g=a[0];
		for(int i=0;i<a.length-1;i++)
		{
			if(a[i+1]>a[i])
			{
				g=a[i+1];
			}
		}
		System.out.println(g);
	}

}
