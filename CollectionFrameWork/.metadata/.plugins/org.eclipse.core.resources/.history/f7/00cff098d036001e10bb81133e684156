package com.sy;

public class MainPro1 {

	public static void main(String[] args) {
		int count = 0;
		for (int n = 1; n <= 100; n++) 
		{
			int rev = 0;
			int n1 = n;
			while (n >0) 
			{
				int rem = n % 10;
				rev =( rev * 10) + rem;
				n = n / 10;
			}
			if (n1 == rev) {

				count++;
			}
		}

		System.out.println(count);
	}
}
