package com.files;

import java.io.File;
import java.util.Arrays;

public class DeleteaFile 
{
	public static void main(String[] args) {
		String s="Hello";
		byte[] ar=s.getBytes();
		Arrays.toString(ar);
		File f=new File("File.txt");
		if(f.delete())
		{
			 System.out.println("The deleted file is : "
                     + f.getName());
		}
		else
		{
			 System.out.println(
		                "Failed in deleting the file.");
			
		}
	}

}
